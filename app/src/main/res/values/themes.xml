<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DhananjayaNidhi" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/Color9a003e</item>
        <item name="colorPrimaryVariant">@color/Color9a003e</item>
        <item name="colorOnPrimary">@color/Color9a003e</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Color9a003e</item>
        <item name="colorSecondaryVariant">@color/Color9a003e</item>
        <item name="colorOnSecondary">@color/Color9a003e</item>
        <item name="android:statusBarColor">@color/Color9a003e</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">false</item>
        <item name="android:windowBackground">@color/white</item>
        <!-- Customize your theme here. -->
        <item name="android:windowActionBar">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowOptOutEdgeToEdgeEnforcement">true</item>
    </style>

<!--    <style name="Theme.DhananjayaNidhi" parent="Base.Theme.DhananjayaNidhi" />-->

    <style name="CustomAlertDialogStylePopup" parent="android:Theme.Holo.Light.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/DialogAnimation</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>

    <style name="NavDrawerTextStyle" parent="Base.TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_24ssp</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
    </style>

    <style name="CustomAlertDialogStyle_space" parent="android:Theme.Holo.Light.Dialog">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:layout_marginStart">@dimen/_10sdp</item>
        <item name="android:layout_marginEnd">@dimen/_10sdp</item>
        <item name="android:layout_marginTop">@dimen/_10sdp</item>
        <item name="android:layout_marginBottom">@dimen/_10sdp</item>
    </style>

    <style name="TextInputLayoutAppearance" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="boxStrokeWidth">@dimen/_1sdp</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="backgroundColor">@color/white</item>
        <item name="expandedHintEnabled">false</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/_8sdp</item>
        <item name="boxCornerRadiusBottomStart">@dimen/_8sdp</item>
        <item name="boxCornerRadiusTopEnd">@dimen/_8sdp</item>
        <item name="boxCornerRadiusTopStart">@dimen/_8sdp</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Edittext" parent="Widget.Design.TextInputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/Color1A1C1E</item>
        <item name="android:background">@null</item>
        <item name="android:drawablePadding">@dimen/_12sdp</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:padding">@dimen/_15sdp</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="MaterialButton" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">@dimen/_8sdp</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/_10sdp</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="CornerCut" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeTopLeft">20%</item>
        <item name="cornerSizeTopRight">20%</item>
        <item name="cornerSizeBottomLeft">20%</item>
        <item name="cornerSizeBottomRight">20%</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>